{
  "rules": {
    "status": {
      "$userId": {
        ".read": true,
        ".write": "$userId === auth.uid"
      }
    },
    "rooms": {
      "$roomId": {
        "typing": {
          "$userId": {
            ".read": true,
            ".write": "$userId === auth.uid"
          }
        }
      }
    }
  }
}
